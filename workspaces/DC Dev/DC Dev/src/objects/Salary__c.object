    <validationRules>
        <fullName>Account_Number_Change</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Bank_Account_Number__c )</errorConditionFormula>
        <errorDisplayField>Bank_Account_Number__c</errorDisplayField>
        <errorMessage>Bank Account Number cannot be changed once it is set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Account_Number_Length</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
   LEN( Bank_Account_Number__c ) &lt;&gt; 25,
   TRUE,
   FALSE
)</errorConditionFormula>
        <errorDisplayField>Bank_Account_Number__c</errorDisplayField>
        <errorMessage>Please enter a valid bank account number. (Length should be 25)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bank_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
     OR(LEN(Bank_Details__c) == 0,LEN(Bank_Account_Number__c) ==0),
   TRUE,
   FALSE
)</errorConditionFormula>
        <errorMessage>Bank Details cannot be Null.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Salary_Date</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    Salary_Credit_Date__c &lt;  Employee__r.Date_of_Joining__c,
    TRUE,
    FALSE
)</errorConditionFormula>
        <errorDisplayField>Salary_Credit_Date__c</errorDisplayField>
        <errorMessage>The salary credit date cannot come before Joining Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Salary_for_Inactive_Employees</fullName>
        <active>true</active>
        <description>Salary cannot be processed for inactive employees.</description>
        <errorConditionFormula>Employee__r.Is_Active__c == FALSE</errorConditionFormula>
        <errorMessage>Salary cannot be processed for inactive employees.</errorMessage>
    </validationRules>
</CustomObject>
