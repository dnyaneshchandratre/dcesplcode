        <active>true</active>
        <errorConditionFormula>IF( 
OR(Date_of_Joining__c &gt;  Last_Working_Day__c, Resignation_Date__c &gt; Last_Working_Day__c), 
TRUE, 
FALSE)</errorConditionFormula>
        <errorMessage>Last working day cannot be before resignation date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Notice_Period_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF( 
   AND(Is_Active__c == FALSE,Is_on_Notice_Period__c == TRUE), 
   TRUE, 
   FALSE
)</errorConditionFormula>
        <errorDisplayField>Is_Active__c</errorDisplayField>
        <errorMessage>The employee cannot be inactive in notice period.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Resignation_Date</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    Date_of_Joining__c  &gt;  Resignation_Date__c ,
   TRUE,
   FALSE)</errorConditionFormula>
        <errorDisplayField>Resignation_Date__c</errorDisplayField>
        <errorMessage>Employee cannot resign before joining.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Resignation_Date_Status</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    AND(Is_on_Notice_Period__c == TRUE, TEXT(Resignation_Date__c) == NULL),
   TRUE,
   FALSE
)</errorConditionFormula>
        <errorDisplayField>Resignation_Date__c</errorDisplayField>
        <errorMessage>You need to enter a resignation date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Resignation_Status</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    AND(Is_on_Notice_Period__c == FALSE, TEXT(Resignation_Date__c) &lt;&gt; NULL),
   TRUE,
   FALSE
)</errorConditionFormula>
        <errorDisplayField>Resignation_Date__c</errorDisplayField>
        <errorMessage>The &quot;Is on Notice Period&quot; checkbox should be checked in order to enter resignation date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SSN_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>LEN( TEXT(SSN__c )) &lt;&gt; 10</errorConditionFormula>
        <errorDisplayField>SSN__c</errorDisplayField>
        <errorMessage>Invalid Value in SSN.</errorMessage>
    </validationRules>
</CustomObject>
