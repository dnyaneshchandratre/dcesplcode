        <fullName>Status__c</fullName>
        <description>Red Flag: Inactive.
Yellow Flag: On notice period.
Green Flag: Active.</description>
        <externalId>false</externalId>
        <formula>IF(
  AND(Is_Active__c == TRUE,Is_on_Notice_Period__c == FALSE),
  IMAGE( &quot;/img/samples/flag_green.gif&quot;,&quot;Employee is active.&quot; ) ,  
  IF( 
    AND(Is_Active__c == TRUE,Is_on_Notice_Period__c == TRUE), 
    IMAGE( &quot;/img/samples/flag_yellow.gif&quot;,&quot;Employee is currently on notice period.&quot;)  ,
    IF( 
      AND(Is_Active__c == FALSE,Is_on_Notice_Period__c == TRUE),  
      NULL,
      IMAGE( &quot;/img/samples/flag_red.gif&quot;,&quot;Employee is currently inactive.&quot;)
    ) 
  )
)</formula>
        <inlineHelpText>Red Flag: Inactive.
Yellow Flag: On notice period.
Green Flag: Active.</inlineHelpText>
        <label>Status</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
